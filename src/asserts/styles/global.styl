ul,li
  list-style-type none
  padding 0
  margin 0
textarea,input
  border none
  margin: 0;
  padding 0
  outline none
a
  color #000
  cursor pointer
  text-decoration none
.gray{
  background-color #f5f5f5
}
.slideout-open .menu{
    display block
}
.menu
  background url("../asserts/images/tab-bg.jpg") 0 0 no-repeat
  width 110px
  position absolute
  left 0
  top 0
  bottom 0
  display none
  li
    position relative
    font-size 15px
    line-height:60px
    border-bottom:1px solid #d3e1ea
    background-color rgba(29,70,109,.6)
    a
      width 100%
      height 100%
      text-indent 35px
      display block
      color #fff
    span
      position: absolute
      left 12px
      top: 20px;
    &.active
      background-color rgba(36,82,125,.6)
.icon-menu{
  pointer-events: none
}
#header
  background-color #fff
  height 50px
  line-height 50px
  position relative
  text-align center
  font-size 20px
  color #28303b
  font-weight bold
  padding 0 50px
  te()
.view
  position absolute
  height 100%
  width 100%
  transition opacity .2s ease
  box-sizing border-box
  overflow-y scroll
  overflow-x hidden
  &.v-enter, &.v-leave
    opacity 0
section{
  background-color #fff
}
.menu_icon{
  position: relative;
  left 20px
  top 15px
  z-index 999
  margin-bottom -15px
}
.banner
  position: relative;
  img
    width 100%
    vertical-align bottom
.wrapper{
  position: relative;
  height 100%
}
.gltk-list .w20{
  padding 14px 0
}
.gltk-list .w20>div{
  margin 0 auto
}
.gltk-list .center,.hxys-list .center{
  position relative
}
.gltk-list .center>div,.hxys-list .center>div{
  center()
}
.swiper-wrap > div {
  display: flex;
  justify-content: center;
  align-items: center;
  text-align: center;
  color: #fff;
}
.swiper img{
  width 100%
  height 90px
}
.list{
  padding 0 12px
  border 1px solid #f0f0f0
}
.container{
  padding 0 12px
}
.menu-list{
  padding 10px 0
  background-color #fff
}
.menu-list p{
  margin: 0 auto;
}
.section-title{
  text-indent 10px;
  line-height:30px;
  position: relative;
}
.section-title:before{
  content:"|"
  color:#4095ff
  position: absolute;
  left: -10px;
  font-size:16px
  font-weight:bold
}
.list-item{
  padding: 8px 0;
  border-top 1px solid #f0f0f0
}
.list-item:first-child{
  border-top 0
}
.list-item-content{
  padding 10px 0
}
.hxys-list .w30{
  padding 20px 0
}
.hxys-list .w30>div{
  float:right
}
.detail{
  border 1px solid #4095ff
  line-height 25px
  text-align center
  border-radius 3px
  color #4095ff
  font-size 12px
  margin-top 10px
  display inline-block
  padding 0 10px
  float right
}
.detail a{
  color inherit
}
.footer{
  background-color #222222
}
.footer ul{
  padding 15px 0
}
.footer li{
  position: relative;
}
.footer li:after{
  content:""
  position: absolute;
  left 0
  top: 0;
  height 34px
  width 1px
  background-color #ebebeb
}
.footer li:first-child:after{
  content:""
  width 0
}
.footer li div{
  margin 0 auto
  position: relative;
}
.copyright{
  border-top 1px solid #3d3f41
  padding 15px 0
}
.download-btn{
  position: absolute;
  bottom 40px
  left 0
  right: 0;
}
.download-btn .ios-btn,.download-btn .andriod-btn,{
  width 123px
  height 39px
  line-height 39px
  display inline-block
  border-radius 5px
  border 1px solid #fff
}
.download-btn .ios-btn{
  background-color #fff
  color #4095ff
  margin-right 10px
  border none
}
.icon-ios,.icon-andriod,.icon-ckgd{
  display inline-block
  vertical-align middle
  margin-right 5px
}
.icon-ckgd{
  margin-left 10px
}
.detailpage{
  z-index 9999
}
.detailinfo{
  padding 0 20px
}
.detailinfo .title{
  color #1967b1
}